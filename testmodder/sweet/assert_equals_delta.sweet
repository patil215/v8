syntax assertEqualsDelta = function (ctx) {
    let paramCtx = ctx.contextify(ctx.next().value);

    let left = paramCtx.expand('expr').value;
    paramCtx.next();
    let right = paramCtx.expand('expr').value;
    paramCtx.next();
    let delta = paramCtx.expand('expr').value;

    return #`assertEqualsDeltaF(() => ${left}, () => ${right}, ${delta})`;
};